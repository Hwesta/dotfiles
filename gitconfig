[core]
	excludesfile = ~/.gitignore_global
	attributesfile = ~/.gitattributes
	commentChar = auto
[alias]
	st = status
	co = checkout
	br = branch
	sw = switch
	unstage = restore --staged
	diffc = diff --staged
	stl = stash list
	delbranch = push origin --delete
	commitfix = commit --fixup
	rbc = rebase --continue
	rbab = rebase --abort
	rbasq = rebase -i --autosquash
[fetch]
	prune = true
[pull]
	rebase = true
[push]
	default = current
[branch]
	autosetuprebase = always
[help]
	autocorrect = 3
[commit]
	verbose = true
[merge]
	conflictstyle = diff3
